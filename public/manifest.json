{
  "name": "Free Press",
  "description": "Finds you alternatives to paywalled news articles",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "./static/js/content.js"
      ]
    }
  ],
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "16": "logo2.png",
    "48": "logo2.png",
    "128": "logo2.png"
  }
}